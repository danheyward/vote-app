<div class='row'>
  <form action='/auth/signup' method='POST'>
      <div class='row'>
        <div class='input-field col s12 m6 l4'>
          <input id='name' class='validate center-align' type='text' name='name' placeholder='Jane Doe'>
          <label for='name'>Name</label>
        </div>

        <div class='input-field col s12 m6 l4'>
          <input id='email' class='validate center-align' type='email' name='email' placeholder='jane@gmail.com'>
          <label for='email'>Email</label>
        </div>

        <div class='input-field col s12 m6 l4'>
          <input id='password' class='validate center-align' type='password' name='password' placeholder='********'>
          <label for='password'>Password</label>
        </div>
      </div>

      <div class='row'>
        <div class='input-field col s12 m6 l4'>
          <input id='address' class='validate center-align' type='text' name='address' placeholder='123 Main Street'>
          <label for='address'>Address</label>
        </div>
      </div>

      <div class='row'>
        <div class='input-field col s12 m6 l4'>
          <input id='address2' class='validate center-align' type='text' name='address2' placeholder='Apartment Name, Unit #'>
          <label for='address2'>Address 2</label>
        </div>

        <div class='input-field col s12 m6 l4'>
          <input id='city' class='validate center-align' type='text' name='city' placeholder='Town City'>
          <label for='city'>City</label>
        </div>

        <div class='input-field col s12 m6 l4'>
          <select id='state' name='state'>
            <option value='' disabled selected>Your State</option>
            <option value='AL'>AL</option><option value='AK'>AK</option><option value='AZ'>AZ</option><option value='AR'>AR</option>
            <option value='CA'>CA</option><option value='CO'>CO</option><option value='CT'>CT</option><option value='DE'>DE</option>
            <option value='FL'>FL</option><option value='GA'>GA</option><option value='HI'>HI</option><option value='ID'>ID</option>
            <option value='IL'>IL</option><option value='IN'>IN</option><option value='IA'>IA</option><option value='KS'>KS</option>
            <option value='KY'>KY</option><option value='LA'>LA</option><option value='ME'>ME</option><option value='MD'>MD</option>
            <option value='MA'>MA</option><option value='MI'>MI</option><option value='MN'>MN</option><option value='MS'>MS</option>
            <option value='MO'>MO</option><option value='MT'>MT</option><option value='NE'>NE</option><option value='NV'>NV</option>
            <option value='NH'>NH</option><option value='NJ'>NJ</option><option value='NM'>NM</option><option value='NY'>NY</option>
            <option value='NC'>NC</option><option value='ND'>ND</option><option value='OH'>OH</option><option value='OK'>OK</option>
            <option value='OR'>OR</option><option value='PA'>PA</option><option value='RI'>RI</option><option value='SC'>SC</option>
            <option value='SD'>SD</option><option value='TN'>TN</option><option value='TX'>TX</option><option value='UT'>UT</option>
            <option value='VT'>VT</option><option value='VA'>VA</option><option value='WA'>WA</option><option value='WI'>WI</option>
            <option value='WV'>WV</option><option value='WY'>WY</option>
          </select>
          <label for='state'>State</label>
        </div>

        <div class='input-field col s4'>
          <input id='zipcode' class='validate center-align' type='text' name='zipcode' placeholder='12345'>
          <label for='zipcode'>Zipcode</label>
        </div>
      </div>

  <div class='row'>
    <div class='input-field col s4'>
      <select id='party' name='party'>
        <option value='' disabled selected>Select your party</option>
        <option value='Democrat'>Democrat</option>
        <option value='Republican'>Republican</option>
        <option value='Independent'>Independent</option>
        <option value='Other'>Other</option>
        <option value='N/A'>N/A</option>
      </select>
      <label for='party'>Party</label>
    </div>
  </div>

  <input class="btn btn-primary" type="submit">
</form>
